{
  "name": "mapping-style-guide-rn",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "npx jest",
    "test:ci": "npx jest --ci --runInBand",
    "test:coverage": "npx jest --coverage && open coverage/lcov-report/index.html",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:ci": "npx eslint . --ext .js,.jsx,.ts,.tsx",
    "tsc": "npx tsc --noEmit",
    "lint:tsc:ci": "run-p lint:ci tsc",
    "upgrade-app-modules": "npx upgrade-app-modules",
    "sync-with-core": "npx sync-with-core",
    "pod-install": "npx --yes pod-install"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.71.7"
  },
  "devDependencies": {
    "@tsconfig/react-native": "2.0.2",
    "@types/jest": "29.2.1",
    "@types/react": "18.0.24",
    "@types/react-test-renderer": "18.0.0",
    "babel-jest": "29.2.1",
    "babel-preset-rn-mapping": "git+ssh://git@github.com-pessoal:williamcamasil/babel-preset-rn-mapping.git#dev",
    "eslint-config-rn-mapping": "git+ssh://git@github.com-pessoal:williamcamasil/eslint-config-rn-mapping.git#dev",
    "jest": "29.2.1",
    "mapping-context-rn": "git+ssh://git@github.com-pessoal:williamcamasil/mapping-context-rn.git#dev",
    "mapping-tools-rn": "git+ssh://git@github.com-pessoal:williamcamasil/mapping-tools-rn.git#dev",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.9.5"
  },
  "jest": {
    "preset": "react-native"
  }
}
